{
    "name": "bernardomacedo/laravel-db-translator",
    "description": "Laravel 5.6 database translation extension",
    "keywords": [
        "bernardomacedo",
        "laravel-db-translator",
        "laravel",
        "translation"
    ],
    "homepage": "https://github.com/bernardomacedo/laravel-db-translator",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernardo Sousa de Macedo",
            "email": "bernardo.macedo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "8.x",
        "illuminate/translation": "8.x",
        "illuminate/database": "8.x",
        "illuminate/console": "8.x",
        "illuminate/filesystem": "8.x",
        "illuminate/config": "8.x",
        "illuminate/routing": "8.x"
    },
    "extra": {
        "laravel": {
            "providers": [
                "bernardomacedo\\DBTranslator\\DBTranslatorServiceProvider"
            ],
            "aliases": {
                "DBTranslator": "bernardomacedo\\DBTranslator\\DBTranslatorFacade"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit" : "^9",
        "orchestra/testbench": "6.x",
        "mockery/mockery": "^1.4.4"
    },
    "autoload": {
        "psr-4": {
            "bernardomacedo\\DBTranslator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bernardomacedo\\DBTranslator\\Test\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/bernardomacedo/laravel-db-translator/issues",
        "source": "https://github.com/bernardomacedo/laravel-db-translator"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
